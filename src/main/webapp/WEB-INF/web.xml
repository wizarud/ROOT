<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>wayOS Play UI</display-name>
<!-- 	<listener> -->
<!-- 		<listener-class>listener.AppListener</listener-class> -->
<!-- 	</listener> -->
	<servlet>
		<servlet-name>VesionCheckerServlet</servlet-name>
		<display-name>VesionCheckerServlet</display-name>
		<description></description>
		<servlet-class>servlet.VesionCheckerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QRCodeServlet</servlet-name>
		<display-name>QRCodeServlet</display-name>
		<description></description>
		<servlet-class>servlet.QRCodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VesionCheckerServlet</servlet-name>
		<url-pattern>/VesionChecker</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QRCodeServlet</servlet-name>
		<url-pattern>/QRCodeServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>401</error-code>
		<location>/ErrorPage.jsp</location>
	</error-page>
	<error-page>
		<error-code>403</error-code>
		<location>/ErrorPage.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/ErrorPage.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/ErrorPage.jsp</location>
	</error-page>
</web-app>
